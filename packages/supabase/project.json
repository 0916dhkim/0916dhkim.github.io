{
  "root": "packages/supabase",
  "sourceRoot": "packages/supabase/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/supabase/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/supabase"
      ],
      "options": {
        "jestConfig": "packages/supabase/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
