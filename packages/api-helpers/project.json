{
  "root": "packages/api-helpers",
  "sourceRoot": "packages/api-helpers/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/api-helpers",
        "main": "packages/api-helpers/src/index.ts",
        "tsConfig": "packages/api-helpers/tsconfig.lib.json",
        "assets": [
          "packages/api-helpers/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/api-helpers/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/api-helpers"
      ],
      "options": {
        "jestConfig": "packages/api-helpers/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
